{
	"name": "martin/customlastpost",
	"type": "phpbb-extension",
	"description": "Customize if the Last Post link on the board index displays the subject of the last post, the title of the topic containing the last post, or the post subject if it is not empty and the topic title otherwise.",
	"homepage": "https://github.com/Martin-G-/CustomLastPost",
	"version": "1.0.0-beta1",
	"time": "2014-12-31",
	"license": "GPL-2.0",
	"authors": [{
		"name": "Martin",
		"email": "martin.g@posteo.de",
		"role": "Lead Developer"
	}],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"extra": {
		"display-name": "Custom Last Post",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0-RC2,<3.2.*@dev"
		},
		"version-check": {
			"host": "martin-g-.github.io",
			"directory": "/versioncheck",
			"filename": "customlastpost.json"
		}
	}
}